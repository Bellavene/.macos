{
    "autosu": true,
    "colorscheme": "terminal",
    "hlsearch": true,
    "infobar": false,
    "parsecursor": true,
    "paste": true,
    "pluginrepos": [
        "https://raw.githubusercontent.com/samdmarshall/micro-fzf-plugin/master/repo.json"
    ],
    "relativeruler": true,
    "rmtrailingws": true,
    "savecursor": true,
    "saveundo": true,
    "softwrap": true,
    "statusformatl": "$(filename) $(modified)($(line),$(col))",
    "statusformatr": "$(status.paste)| ft:$(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
    "tabmovement": true,
    "tabstospaces": true,
    "terminal": "iTerm2",
    "wordwrap": true,
    "xterm": true
}
